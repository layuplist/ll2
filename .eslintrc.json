{
  "root": true,
  "extends": [
    "eslint:recommended",
    "plugin:@typescript-eslint/recommended"
  ],
  "parser": "@typescript-eslint/parser",
  "plugins": [
    "@typescript-eslint"
  ],
  "ignorePatterns": [
    "node_modules",
    "cdk.out",
    "dist"
  ],
  "rules": {
    "@typescript-eslint/no-non-null-assertion": 0
  }
}
